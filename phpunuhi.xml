<phpunuhi
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../boxblinkracer/phpunuhi/config.xsd"
>
    <translations>
        <set name="Backend">
            <rules>
                <nestingDepth>3</nestingDepth>
            </rules>
            <styles>
                <style>upper</style>
                <style>snake</style>
            </styles>
            <format>
                <php/>
            </format>
            <locales>
                <locale name="de">./Application/views/%locale_lc%/d3_ordermanager_translations.php</locale>
                <locale name="en">./Application/views/%locale_lc%/d3_ordermanager_translations.php</locale>
            </locales>
        </set>
    </translations>
</phpunuhi>